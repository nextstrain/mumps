{
  "title": "Real-time tracking of mumps virus evolution",
  "maintainers": [
    {"name": "the Nextstrain team", "url": "https://nextstrain.org/team"}
  ],
  "data_provenance": [
    {
      "name": "GenBank",
      "url": "https://www.ncbi.nlm.nih.gov/genbank/"
    }
  ],
  "build_url": "https://github.com/nextstrain/mumps",
  "colorings": [
    {
      "key": "gt",
      "title": "Genotype",
      "type": "categorical"
    },
    {
      "key": "num_date",
      "title": "Date",
      "type": "continuous"
    },
    {
      "key": "author",
      "title": "Author",
      "type": "categorical"
    },
    {
      "key": "MuV_genotype",
      "title": "MuV genotype (GenBank)",
      "type": "categorical"
    },
    {
      "key": "MuV_genotype_nextclade_sh",
      "title": "MuV genotype SH (Nextclade)",
      "type": "categorical"
    },
    {
      "key": "MuV_genotype_nextclade_genome",
      "title": "MuV genotype genome (Nextclade)",
      "type": "categorical"
    },
    {
      "key": "MuV_genotype_nextclade_genomesample",
      "title": "MuV genotype genome sampled (Nextclade)",
      "type": "categorical"
    },
    {
      "key": "MuV_genotype_nextclade_genomesampleoutgroup",
      "title": "MuV genotype genome sampled outgrouped (Nextclade)",
      "type": "categorical"
    },
    {
      "key": "country",
      "title": "Country",
      "type": "categorical"
    },
    {
      "key": "region",
      "title": "Region",
      "type": "categorical"
    }
  ],
  "geo_resolutions": [
    "country",
    "region"
  ],
  "panels": [
    "tree",
    "map",
    "entropy",
    "frequencies"
  ],
  "display_defaults": {
    "map_triplicate": true,
    "color_by": "country",
    "tip_label": "strain"
  },
  "filters": [
    "MuV_genotype",
    "country",
    "region",
    "author"
  ],
  "metadata_columns": [
    "strain",
    "isolate",
    "host"
  ],
  "extensions": {
    "nextclade": {
      "clade_node_attrs": [
        {
          "name": "MuV_genotype",
          "displayName": "GenBank",
          "description": "Mumps genotype based on GenBank"
        },
        {
          "name": "MuV_genotype_nextclade_sh",
          "displayName": "Nextclade SH",
          "description": "Mumps genotype based on SH region"
        },
        {
          "name": "MuV_genotype_nextclade_genome",
          "displayName": "Nextclade Genome",
          "description": "Mumps genotype based on Genome region"
        },
        {
          "name": "MuV_genotype_nextclade_genomesample",
          "displayName": "Nextclade Genome Sampled",
          "description": "Mumps genotype based on Genome + sampled region"
        },
        {
          "name": "MuV_genotype_nextclade_genomesampleoutgroup",
          "displayName": "Nextclade Genome Sampled with Outgroup",
          "description": "Mumps genotype based on Genome + sampled region + outgroup"
        }
      ],
      "pathogen": {
        "schemaVersion":"3.0.0",
        "attributes": {
          "name": "Mumps virus global tree",
          "reference name": "Reconstructed ancestor from global tree",
          "reference accession": "none"
        }
      }
    }
  }
}
